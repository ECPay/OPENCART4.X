{{ header}}{{ column_left}}
<div id="content">
    <div class="page-header">
        <div class="container-fluid">
            <div class="float-end">
                <button type="submit" form="form-ecpaylogistic" data-bs-toggle="tooltip" title="{{ button_save }}" class="btn btn-primary">
                    <i class="fa fa-save"></i>
                </button>
                <a href="{{ cancel }}" data-bs-toggle="tooltip" title="{{ button_cancel }}" class="btn btn-light">
                    <i class="fa fa-reply"></i>
                </a>
            </div>

            <h1>{{ heading_title }}</h1>

            <ul class="breadcrumb">
                {% for breadcrumb in breadcrumbs %}
                <li class="breadcrumb-item"><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
                {% endfor %}
            </ul>
        </div>
    </div>
    <div class="container-fluid">
        {% if error_warning %}
        <div class="alert alert-danger">
            <i class="fa fa-exclamation-circle"></i> {{ error_warning }}
            <button type="button" class="close" data-dismiss="alert">&times;</button>
        </div>
        {% endif %}
        <div class="card">
            <div class="card-header">
                <i class="fa fa-pencil"></i> 
                {{ text_edit }}
            </div>

            <div class="card-body">
                <form action="{{ action }}" method="post" data-oc-toggle="ajax" id="form-ecpaylogistic">
                    <div class="row">
                        <div class="col-sm-2">
                            <!--新的-->
                            <p>
                                <!--一般設定-->
                                <button class="btn btn-light mb-2 w-100" type="button" data-bs-toggle="collapse" data-bs-target="#tab-general" aria-expanded="true" aria-controls="tab-general">{{ text_general }}</button>
                                <!--統一超商取貨-->
                                <button class="btn btn-light mb-2 w-100" type="button" data-bs-toggle="collapse" data-bs-target="#tab-unimart" aria-expanded="false" aria-controls="tab-unimart">{{ text_unimart }}</button>
                                <!--統一超商取貨付款-->
                                <button class="btn btn-light mb-2 w-100" type="button" data-bs-toggle="collapse" data-bs-target="#tab-unimart-collection" aria-expanded="false" aria-controls="tab-unimart-collection">{{ text_unimart_collection }}</button>
                                <!--全家超商取貨-->
                                <button class="btn btn-light mb-2 w-100" type="button" data-bs-toggle="collapse" data-bs-target="#tab-fami" aria-expanded="false" aria-controls="tab-fami">{{ text_fami }}</button>
                                <!--全家超商取貨付款-->
                                <button class="btn btn-light mb-2 w-100" type="button" data-bs-toggle="collapse" data-bs-target="#tab-fami-collection" aria-expanded="false" aria-controls="tab-fami-collection">{{ text_fami_collection }}</button>
                                <!--萊爾富超商取貨-->
                                <button class="btn btn-light mb-2 w-100" type="button" data-bs-toggle="collapse" data-bs-target="#tab-hilife" aria-expanded="false" aria-controls="tab-hilife">{{ text_hilife }}</button>
                                <!--萊爾富超商取貨付款-->
                                <button class="btn btn-light mb-2 w-100" type="button" data-bs-toggle="collapse" data-bs-target="#tab-hilife-collection" aria-expanded="false" aria-controls="tab-hilife-collection">{{ text_hilife_collection }}</button>
                                <!--OK超商取貨-->
                                <button class="btn btn-light mb-2 w-100" type="button" data-bs-toggle="collapse" data-bs-target="#tab-okmart" aria-expanded="false" aria-controls="tab-okmart">{{ text_okmart }}</button>
                                <!--OK超商取貨付款-->
                                <button class="btn btn-light mb-2 w-100" type="button" data-bs-toggle="collapse" data-bs-target="#tab-okmart-collection" aria-expanded="false" aria-controls="tab-okmart-collection">{{ text_okmart_collection }}</button>
                                <!--中華郵政-->
                                <button class="btn btn-light mb-2 w-100" type="button" data-bs-toggle="collapse" data-bs-target="#tab-post" aria-expanded="false" aria-controls="tab-post">{{ text_post }}</button>
                                <!--黑貓物流-->
                                <button class="btn btn-light mb-2 w-100" type="button" data-bs-toggle="collapse" data-bs-target="#tab-tcat" aria-expanded="false" aria-controls="tab-tcat">{{ text_tcat }}</button>
                            </p>
                        </div>
                        <div id="content-group" class="col-sm-10">
                            <!--一般設定-->
                            <div class="row">
                                <div class="collapse multi-collapse show" data-bs-parent="#content-group" id="tab-general">
                                    <!--Status-->
                                    <div class="row mb-3">
                                        <label class="col-sm-2 col-form-label" for="input-shipping-ecpaylogistic-status">
                                            <span data-toggle="tooltip" title="{{ entry_status }}">
                                                {{ entry_status }}
                                            </span>
                                        </label>
                                        <div class="col-sm-10">
                                            <div class="form-check form-switch form-switch-lg">
                                                <input type="hidden" name="shipping_ecpaylogistic_status" value="0"/>
                                                <input type="checkbox" name="shipping_ecpaylogistic_status" value="1" id="input-status" class="form-check-input" {% if shipping_ecpaylogistic_status %} checked {% endif %}/>
                                            </div>
                                            <div id="error-shipping-ecpaylogistic-status" class="invalid-feedback"></div>
                                        </div>
                                    </div>
                                    <!--MID-->
                                    <div class="row mb-3 required">
                                        <label class="col-sm-2 col-form-label" for="input-shipping-ecpaylogistic-mid">
                                            <span data-toggle="tooltip" title="{{ entry_mid }}">
                                                {{ entry_mid }}
                                            </span>
                                        </label>
                                        <div class="col-sm-10">
                                            <input type="text" name="shipping_ecpaylogistic_mid" value="{{ shipping_ecpaylogistic_mid }}" placeholder="{{ entry_mid }}" id="input-shipping-ecpaylogistic-mid" class="form-control" />
                                            <div id="error-shipping-ecpaylogistic-mid" class="invalid-feedback"></div>
                                        </div>
                                    </div>
                                    <!--Hash Key-->
