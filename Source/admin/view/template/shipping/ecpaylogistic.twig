                                            <span data-toggle="tooltip" title="{{ entry_MaxAmount }}">
                                                {{ entry_MaxAmount }}
                                            </span>
                                        </label>
                                        <div class="col-sm-10">
                                            <input type="text" name="shipping_ecpaylogistic_max_amount" value="{{ shipping_ecpaylogistic_max_amount }}" placeholder="{{ entry_MaxAmount }}" id="input-shipping-ecpaylogistic-max-amount" class="form-control" />
                                            <div id="error-shipping-ecpaylogistic-max-amount" class="invalid-feedback"></div>
                                        </div>
                                    </div>
                                    <!--Geo Zone-->
                                    <div class="row mb-3 required">
                                        <label class="col-sm-2 col-form-label" for="input-shipping-ecpaylogistic-geo-zone-id">
                                            <span data-toggle="tooltip" title="{{ entry_geo_zone }}">
                                                {{ entry_geo_zone }}
                                            </span>
                                        </label>
                                        <div class="col-sm-10">
                                            <select name="shipping_ecpaylogistic_geo_zone_id" id="input-geo-zone" class="form-control">
                                                <option value="0">{{ text_all_zones }}</option>
                                                {% for geo_zone in geo_zones %}
                                                {% if geo_zone.geo_zone_id == shipping_ecpaylogistic_geo_zone_id %}
                                                <option value="{{ geo_zone.geo_zone_id }}" selected="selected">{{ geo_zone.name }}</option>
                                                {% else %}
                                                <option value="{{ geo_zone.geo_zone_id }}">{{ geo_zone.name }}</option>
                                                {% endif %}
                                                {% endfor %}
                                            </select>
                                            <div id="error-shipping-ecpaylogistic-geo-zone-id" class="invalid-feedback"></div>
                                        </div>
                                    </div>
                                    <!--Order Status-->   
                                    <div class="row mb-3 required">
                                        <label class="col-sm-2 col-form-label" for="input-shipping-ecpaylogistic-order-status">
