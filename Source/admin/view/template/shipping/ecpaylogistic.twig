                                                        <option value="{{ ecpayl_type.value }}" selected="selected">{{ ecpayl_type.text }}</option>
                                                    {% else %}
                                                        <option value="{{ ecpayl_type.value }}">{{ ecpayl_type.text }}</option>
                                                    {% endif %}
